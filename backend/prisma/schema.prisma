generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id            String         @id
  username      String
  icon_url      String
  status        String

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}